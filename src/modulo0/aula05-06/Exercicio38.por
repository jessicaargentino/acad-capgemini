programa
{
	
	funcao inicio()
	{
		inteiro A, B
		
		escreva("Digite o valor de A: ")
		leia(A)
		escreva("Digite o valor de B: ")
		leia(B)

		escreva("\nO resultado da subtração de A - B é: ", realizaSubtracao(A,B))
	}
	
	funcao inteiro realizaSubtracao(inteiro A, inteiro B){
		retorne (A - B)
	}
}
