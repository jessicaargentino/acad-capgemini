programa
{
	
	funcao inicio()
	{
		inteiro A, quadrado = 0
		
		escreva("Digite o valor de A: ")
		leia(A)
		quadrado = (A * A)
		escreva("O quadrado do valor digitado é: " , quadrado)
	}
}
