programa
{
	
	funcao inicio()
	{
		inteiro A, B, quadradoA = 0, quadradoB = 0, somaQuadrados = 0
		
		escreva("Digite o valor de A: ")
		leia(A)
		escreva("Digite o valor de B: ")
		leia(B)
		
		quadradoA = (A * A)
		quadradoB = (B * B)
		somaQuadrados = (quadradoA + quadradoB)
		
		escreva("A soma dos quadrados dos valores digitados é: " , somaQuadrados)
	}
}
